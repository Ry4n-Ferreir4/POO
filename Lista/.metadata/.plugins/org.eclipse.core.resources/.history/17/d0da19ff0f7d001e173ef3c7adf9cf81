package bairro;

public class Pessoa {
	 private String nome;
	    private Endereco endereco;

	    public Pessoa(String nome, Endereco endereco) {
	        this.nome = nome;
	        this.endereco = endereco;
	    }

	    // Getter para nome
	    public String getNome() {
	        return nome;
	    }

	    // Getter para o objeto 'Endereco'
	    public Endereco getEndereco() {
	        return endereco;
	    }
}
